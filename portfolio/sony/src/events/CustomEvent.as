package src.events // SonyCurtain{	import flash.events.Event;		public class CustomEvent extends Event 	{		public static const CHANGE:String="onChange";		public static const COMPLETE:String="onComplete";		public static const IMAGE_LOADED:String="onImageLoaded";		public static const SWF_LOADED:String="onSWFLoaded";		public static const SWF_UNLOAD:String="onSWFUnload";		public static const CLICK:String="onClick";		public static const PAUSE_MOVIE:String="onPause"; // pauses Video		public static const PLAY_MOVIE:String="onPlay"; // unpauses Video		public static const MOVIE_ENDS:String="movieEnds"; // When the video ends by itself				public var params:Object;				public function CustomEvent(type:String, params:Object, bubbles:Boolean = false, cancelable:Boolean = false)		{			super(type, bubbles, cancelable);			this.params=params;		}				public override function clone():Event 		{			return new CustomEvent(type, this.params, bubbles, cancelable);		}				public override function toString():String 		{			return formatToString("CustomEvent","params","type","bubbles","cancelable");		}			}	}